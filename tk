import tkinter as tk    
form = tk.Tk()
#form.title() = parantezin içine ne yazarsanız başlık yerine yazılır 
form.title( "tkinter ders 3" )
form.geometry("500x500+100+100")
#max size = pencerenin ne kadar büyütülebileciğini söyler
#min size = pencerenin ne kadar küçültülebileciğini söyler
form.maxsize(500,500)
form.minsize(250,250)
form =tk.Label(form,text="DERS 3")
form.mainloop()
